<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <Version>1.1.0</Version>
    <Company>Fenix Alliance Inc.</Company>
    <Authors>Fenix Alliance Group</Authors>
    <NeutralLanguage>en-US</NeutralLanguage>
    <TargetFramework>net5.0</TargetFramework>
    <NoDefaultExcludes>true</NoDefaultExcludes>
    <AddRazorSupportForMvc>true</AddRazorSupportForMvc>
    <RequireLicenseAcceptance>true</RequireLicenseAcceptance>
    <PackageProjectUrl>https://absuite.net</PackageProjectUrl>
    <PackageLicenseUrl>https://absuite.net/eula</PackageLicenseUrl>
    <Copyright>© 2009 - 2021 Fenix Alliance Inc. All rights reserved.</Copyright>
    <PackageIconUrl>https://fenixalliance.com.co/images/FenixAlliance/AppIcon/apple-icon-120x120.png</PackageIconUrl>
    <RepositoryUrl>https://github.com/FenixAlliance/ABS.Portal</RepositoryUrl>
    <PackageTags>Alliance Business Suite, Fenix Alliance, ABS</PackageTags>
    <Description>Application Component for the Alliance Business Suite.</Description>
    <PackageReleaseNotes>Standarize at v1.1.0</PackageReleaseNotes>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Sendgrid" Version="9.22.0" />
    <PackageReference Include="FenixAlliance.ABM.Hub" Version="1.1.0" />
  </ItemGroup>

  <ItemGroup>
    <None Update="Icon.png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
