<script asp-append-version="true" type="text/javascript"  src="_content/FenixAlliance.ABS.SDK/js/Utils.js"></script>
<script asp-append-version="true" type="text/javascript"  src="_content/Radzen.Blazor/Radzen.Blazor.js"></script>

<script type="text/javascript">
    abs.initForexRates()
